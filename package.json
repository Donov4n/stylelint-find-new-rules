{
  "name": "stylelint-find-rules",
  "version": "1.0.3",
  "description": "Find stylelint rules that you don't have in your config",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "fail": "node index.js --config tests/configs/deprecated-and-new.config.js",
    "install-peers": "install-self-peers",
    "format-prettier": "prettier --write \"**/*.js\"",
    "format-eslint": "eslint -c .eslintrc --fix \"**/*.js\"",
    "format-staged": "prettier-eslint --prettier-last --write",
    "format": "prettier-eslint --prettier-last --write \"**/*.js\"",
    "lint-prettier": "prettier -l \"**/*.js\"",
    "lint-eslint": "eslint -c .eslintrc --max-warnings 0 \"**/*.js\"",
    "lint-eslint-config-prettier": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "npm run lint-eslint-config-prettier && npm run lint-prettier && npm run lint-eslint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "format-staged",
      "git add"
    ]
  },
  "bin": {
    "stylelint-find-rules": "src/bin/stylelint-find-rules.js"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "columnify": "1.5.4",
    "cosmiconfig": "2.2.2",
    "lodash": "4.17.4",
    "yargs": "9.0.1"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": "1.1.1",
    "babel-eslint": "7.2.3",
    "eslint": "4.7.1",
    "eslint-config-prettier": "2.5.0",
    "husky": "0.14.3",
    "lint-staged": "4.2.1",
    "prettier": "1.7.0",
    "prettier-eslint-cli": "4.3.2",
    "stylelint": "8.1.1",
    "stylelint-config-recommended": "1.0.0",
    "stylelint-config-standard": "17.0.0"
  },
  "peerDependencies": {
    "stylelint": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexilyaev/stylelint-find-rules.git"
  },
  "keywords": [],
  "author": "Alex Ilyaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexilyaev/stylelint-find-rules/issues"
  },
  "homepage": "https://github.com/alexilyaev/stylelint-find-rules#readme"
}
