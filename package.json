{
  "name": "stylelint-find-rules",
  "version": "0.6.0",
  "description": "Find Stylelint rules that you don't have in your config",
  "main": "src/bin/stylelint-find-rules.js",
  "scripts": {
    "install-peers": "install-self-peers",
    "format-prettier": "prettier --print-width 100 --single-quote --write \"**/*.js\"",
    "format-eslint": "eslint -c .eslintrc --fix \"**/*.js\"",
    "format": "prettier-eslint --prettier-last --print-width 100 --single-quote --write \"**/*.js\"",
    "lint-prettier": "prettier -l --print-width 100 --single-quote \"**/*.js\"",
    "lint-eslint": "eslint -c .eslintrc --max-warnings 0 \"**/*.js\"",
    "lint-eslint-config-prettier": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "npm run lint-eslint-config-prettier && npm run lint-prettier && npm run lint-eslint",
    "stylelint-find-unused-rules": "node src/bin/stylelint-find-rules.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "format",
      "git add"
    ]
  },
  "bin": {
    "stylelint-find-rules": "src/bin/stylelint-find-rules.js"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "columnify": "1.5.4",
    "cosmiconfig": "2.2.2",
    "lodash.difference": "4.5.0",
    "lodash.intersection": "4.4.0",
    "yargs": "8.0.2"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": "1.1.1",
    "babel-eslint": "7.2.3",
    "eslint": "4.4.1",
    "eslint-config-prettier": "2.3.0",
    "husky": "0.14.3",
    "lint-staged": "4.0.2",
    "prettier": "1.5.3",
    "prettier-eslint-cli": "4.1.1",
    "stylelint": ">=7.0.0"
  },
  "peerDependencies": {
    "stylelint": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexilyaev/stylelint-find-rules.git"
  },
  "keywords": [],
  "author": "Alex Ilyaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexilyaev/stylelint-find-rules/issues"
  },
  "homepage": "https://github.com/alexilyaev/stylelint-find-rules#readme"
}
